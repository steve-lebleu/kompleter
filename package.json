{
  "name": "kompleter",
  "version": "1.0.4",
  "description": "jQuery auto-complete plugin",
  "main": "src/js/index.js",
  "scripts": {
    "build": "mkdir -p dist && cp -r src/index.html src/files/ dist/ & npm run js & npm run css & exit",
    "css": "node-sass ./src/sass/kompleter.scss ./dist/css/kompleter.min.css --output-style compressed",
    "js": "webpack",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome ./cypress",
    "start": "webpack serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/steve-lebleu/kompleter.git"
  },
  "keywords": [
    "html",
    "html5",
    "css",
    "css3",
    "vanilla",
    "javascript",
    "jquery",
    "plugin",
    "auto-complete"
  ],
  "author": "Steve Lebleu",
  "license": "ISC",
  "devDependencies": {
    "@babel/runtime": "^7.23.6",
    "cypress": "^13.6.3",
    "html-loader": "^5.0.0",
    "jquery": "3.1.1",
    "node-sass": "^9.0.0",
    "terser": "^5.26.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-concat-files-plugin": "^0.5.2",
    "webpack-dev-server": "^4.15.1"
  }
}
